{
  "name": "exomiser",
  "title": "exomiser",
  "summary": "exomiser",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "exomiser_template",
      "label": "template",
      "class": "file",
      "patterns": ["*.template"],
      "help": "template file."
    },
    {
      "name": "family_vcf",
      "label": "family vcf file",
      "class": "file",
      "patterns": ["*.vcf"],
      "help": "vcf format file."
    },
    {
      "name": "ped_file",
      "label": "ped file",
      "class": "file",
      "patterns": ["*.ped"],
      "help": "ped format file."
    },
    {
      "name": "python_file",
      "label": "python file",
      "class": "file",
      "patterns": ["*.py"],
      "help": "python file."
    },
    {
      "name": "prop_file",
      "label": "properties file",
      "class": "file",
      "patterns": ["*.properties"],
      "help": "properties file."
    }
  ],
  "outputSpec": [
    {
      "name": "exomiser_yaml",
      "class": "file",
      "label": "yaml file",
      "patterns": [
        "*.yaml"
      ],
      "help": "Output yaml file."
    }
  ],
  "runSpec": {
    "interpreter": "bash",
    "file": "src/exomiser_run.sh",
    "timeoutPolicy": {
      "*": {
        "hours": 3
      }
    },
    "systemRequirements": {
      "*": {
        "instanceType": "azure:mem1_ssd1_x4"
      }
    },
    "distribution": "Ubuntu",
    "release": "16.04"
  }
}
